{
    "buildCommand": "python manage.py migrate && python manage.py collectstatic --noinput",
    "outputDirectory": ".",
    "builds": [
      {
        "src": "config/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "config/wsgi.py"
      }
    ]
  }